<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/transparent"/>
            <size android:height="22dp" android:width="22dp"/>
        </shape>
    </item>
    <item android:left="7dp" android:top="9dp" android:right="6dp" android:bottom="9dp">
        <shape android:shape="rectangle">
            <solid android:color="#ffffff"/>
            <size android:height="4dp" android:width="9dp"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
</layer-list>
