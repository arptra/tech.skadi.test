<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/fd_sys_radius_corner_smooth_large"/>
            <stroke android:width="@dimen/mz_btn_stroke_width" android:color="?attr/colorSurfaceContainerLow" android:dashWidth="2dp" android:dashGap="0dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <corners android:radius="@dimen/fd_sys_radius_corner_smooth_large"/>
                    <stroke android:width="@dimen/mz_btn_stroke_width" android:color="?attr/colorPrimary" android:dashWidth="2dp" android:dashGap="0dp"/>
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <corners android:radius="@dimen/fd_sys_radius_corner_smooth_large"/>
                    <stroke android:width="@dimen/mz_btn_stroke_width" android:color="?attr/colorStatePressed" android:dashWidth="2dp" android:dashGap="0dp"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/fd_sys_radius_corner_smooth_large"/>
            <stroke android:width="@dimen/mz_btn_stroke_width" android:color="?attr/colorPrimary" android:dashWidth="2dp" android:dashGap="0dp"/>
        </shape>
    </item>
</selector>
