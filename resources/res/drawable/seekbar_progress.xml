<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:gravity="center_vertical" android:id="@android:id/background">
        <shape>
            <corners android:radius="20dp"/>
            <solid android:color="#ebebeb"/>
        </shape>
    </item>
    <item android:gravity="center_vertical" android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="20dp"/>
                <gradient android:startColor="#207ffd" android:endColor="#207ffd" android:angle="0"/>
            </shape>
        </clip>
    </item>
</layer-list>
