<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="true" android:state_pressed="true">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="?attr/colorSurfaceContainerLowest"/>
                    <corners android:radius="8dp"/>
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="?attr/colorStatePressed"/>
                    <corners android:radius="8dp"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="?attr/colorSurfaceContainerLowest"/>
            <corners android:radius="8dp"/>
        </shape>
    </item>
</selector>
