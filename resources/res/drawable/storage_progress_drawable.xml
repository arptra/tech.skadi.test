<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape>
            <corners android:radius="11dp"/>
            <solid android:color="#14000000"/>
            <size android:height="18dp"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="11dp"/>
                <size android:height="18dp"/>
                <gradient android:startColor="@color/color_gradient_star" android:endColor="@color/color_gradient_end" android:angle="0"/>
            </shape>
        </scale>
    </item>
</layer-list>
