<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:gravity="center_vertical" android:height="@dimen/mz_seekbar_height">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/mz_seekbar_radius"/>
            <solid android:color="@color/fast_record_seek_bar_bg_color"/>
        </shape>
    </item>
    <item android:gravity="left|center_vertical|center_horizontal|center|start" android:height="@dimen/mz_seekbar_height" android:width="11dp">
        <shape android:shape="rectangle">
            <corners android:topLeftRadius="@dimen/mz_seekbar_radius" android:bottomLeftRadius="@dimen/mz_seekbar_radius"/>
            <solid android:color="@color/fast_record_seek_thumb"/>
        </shape>
    </item>
</layer-list>
