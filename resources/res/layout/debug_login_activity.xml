<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:padding="10dp" android:layout_width="match_parent" android:layout_height="match_parent">
    <TextView android:gravity="center" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="这个页面用户测试登陆相关内容"/>
    <Button android:id="@+id/get_mz_token_btn" android:layout_width="match_parent" android:layout_height="40dp" android:layout_marginTop="10dp" android:text="获取mz token"/>
    <TextView android:id="@+id/mz_token_text" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    <Button android:id="@+id/get_user_info_btn" android:layout_width="match_parent" android:layout_height="40dp" android:layout_marginTop="10dp" android:text="获取用户信息"/>
    <TextView android:id="@+id/user_info_text" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    <View android:background="@color/black" android:layout_width="match_parent" android:layout_height="1px" android:layout_marginTop="10dp"/>
    <Button android:id="@+id/agree_pp_agree_btn" android:layout_width="match_parent" android:layout_height="40dp" android:layout_marginTop="10dp" android:text="同意用户协议"/>
    <Button android:id="@+id/disagree_pp_agree_btn" android:layout_width="match_parent" android:layout_height="40dp" android:layout_marginTop="10dp" android:text="不同意用户协议"/>
    <Button android:id="@+id/query_pp_expire_btn" android:layout_width="match_parent" android:layout_height="40dp" android:layout_marginTop="10dp" android:text="获取用户协议是否过期"/>
    <View android:background="@color/black" android:layout_width="match_parent" android:layout_height="1px" android:layout_marginTop="10dp"/>
    <Button android:id="@+id/request_ai_state_btn" android:layout_width="match_parent" android:layout_height="40dp" android:layout_marginTop="10dp" android:text="请求ai协议是否同意"/>
    <Button android:id="@+id/agree_ai_state_btn" android:layout_width="match_parent" android:layout_height="40dp" android:layout_marginTop="10dp" android:text="同意ai协议"/>
    <Button android:id="@+id/disagree_ai_state_btn" android:layout_width="match_parent" android:layout_height="40dp" android:layout_marginTop="10dp" android:text="取消同意ai协议"/>
    <View android:background="@color/black" android:layout_width="match_parent" android:layout_height="1px" android:layout_marginTop="10dp"/>
    <Button android:id="@+id/query_policys" android:layout_width="match_parent" android:layout_height="40dp" android:layout_marginTop="10dp" android:text="请求所有policy"/>
</LinearLayout>
