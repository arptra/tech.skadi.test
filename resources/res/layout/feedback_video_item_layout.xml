<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="wrap_content" android:layout_height="wrap_content">
    <RelativeLayout android:layout_width="64dp" android:layout_height="64dp">
        <com.upuphone.xr.sapp.view.RoundCornerImageView android:id="@+id/img" android:layout_width="match_parent" android:layout_height="match_parent" android:src="@drawable/ic_feedback_default" android:layout_centerInParent="true" app:radius="8dp"/>
        <TextView android:textSize="12sp" android:textColor="@color/color_ee" android:id="@+id/time_tv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="4dp" android:layout_alignParentRight="true" android:layout_alignParentBottom="true" android:layout_marginEnd="4dp" android:lineHeight="12dp" android:textFontWeight="@integer/medium"/>
        <ImageView android:id="@+id/select_img" android:paddingLeft="14dp" android:paddingBottom="14dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_feedback_video_close" android:layout_alignParentRight="true"/>
    </RelativeLayout>
</LinearLayout>
