<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:tag="layout/layout_double_channel_control_0" android:background="@drawable/rect_radius12_8_f9fafb" android:paddingLeft="15dp" android:paddingRight="15dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:divider="#999999" android:showDividers="middle">
    <TextView android:textSize="16sp" android:gravity="center" android:layout_width="match_parent" android:layout_height="50dp" android:text="通道状态"/>
